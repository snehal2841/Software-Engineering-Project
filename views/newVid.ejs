<%- include("./partials/header") %>

<link rel="stylesheet" href="/css/newVid.css">

<div class="container">
	<div class="row">
		<h1 style="text-align: center">Add new Video</h1>
		<div style="width: 30%; margin: 35px auto">
			<form action="/videos" method="post">
				<div class="form-group">
					<input class="form-control" type="text" placeholder="Video URL ID" name="vid_url_id">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" placeholder="Video ID" name="vid_id">
				</div>
				<div class="form-group">
					<input class="form-control" type="date" placeholder="Date" name="date">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" placeholder="Location" name="location">
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-warning btn-default btn-block">Submit</button>
				</div>
			</form>
		<a href="/videos">Go Back</a>
		</div>
	</div>
</div>

<%- include("./partials/footer") %>